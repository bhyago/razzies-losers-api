DATABASE_URL="postgres://postgres:docker@localhost:5432/banking-ledger"
PORT=3444

# Queue/RabbitMQ (matches docker-compose: user/pass rabbitmq/rabbitmq)
QUEUE_SERVER_URL="amqp://rabbitmq:rabbitmq@localhost:5672"
RABBITMQ_REQUEUE_ON_FAIL=false

# Batch processing
TX_BATCH_SIZE=100
TX_BATCH_WINDOW_MS=200

# Queue names
RABBITMQ_DEPOSIT_QUEUE="tx-deposits"
RABBITMQ_WITHDRAW_QUEUE="tx-withdrawals"
RABBITMQ_TRANSFER_QUEUE="tx-transfers"

# Optional others (unused here, kept for reference)
JWT_PRIVATE_KEY="your_jwt_private_key_here"
JWT_PUBLIC_KEY="your_jwt_public_key_here"
REDIS_PASSWORD="your_redis_password_here"
REDIS_URL="redis://localhost:6379"
